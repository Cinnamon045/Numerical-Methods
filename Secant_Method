def secant_method(function, x0, x1,  n):

  def f(x): 
    f = eval(function)
    return f

  for intercept in range(1,n):
    f_x0 = f(x0)
    f_x1 = f(x1)

    xi = x0 - (f_x0 / ((f_x0 - f_x1)/(x0 - x1)))

    #Update values of x0 and x1

    x0 = x1
    x1 = xi

  print(f"The root was found to be a {xi} after {n} iterations")
